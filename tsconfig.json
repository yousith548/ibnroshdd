{
  "compilerOptions": {
    "jsx": "react",
    "module": "commonjs",
  "target": "es2017",
  "lib": ["dom", "es2015", "es2017"],
  "strict": true,
  "esModuleInterop": true,
  "noUnusedLocals": true,
  "noUnusedParameters": true,
  "removeComments": false,
  "preserveConstEnums": true,
  "outDir": "dist",
  "listEmittedFiles": true
  }, 
  "include": ["./src/**/*"]
}